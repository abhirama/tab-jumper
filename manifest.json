{
  "name": "Tab Jumper",
  "version": "0.1",
  "description": "Name your tabs and jump to them using the keyboard.",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" 
          },
  "permissions": [
    "tabs"
  ],
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.js"]
    }
  ]
}
